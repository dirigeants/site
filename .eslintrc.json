
{
  "extends": ["klasa/eslint-ts", "plugin:vue/essential", "@vue/typescript/recommended"],
  "env": {
    "node": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "new-cap": "off",
    "indent": ["error", "tab", { "SwitchCase": 1 }],
    "no-invalid-this": "off"
  }
}